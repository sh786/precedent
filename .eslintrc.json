{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:tailwindcss/recommended"
  ],
  "plugins": ["prettier", "tailwindcss"],
  "rules": {
    "no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }
    ]
  },
  "settings": {
    "tailwindcss": {
      "callees": ["cn"]
    }
  }
}
